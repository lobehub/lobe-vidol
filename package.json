  "keywords": [
    "framework",
    "virtual-idols",
    "vidol",
    "vtuber",
    "chatbot",
    "chatgpt",
    "nextjs",
    "vercel-ai",
    "openai",
    "tts"
  ],
  "homepage": "https://github.com/lobehub/lobe-vidol",
  "bugs": {
    "url": "https://github.com/lobehub/lobe-vidol/issues/new/choose"
  },
